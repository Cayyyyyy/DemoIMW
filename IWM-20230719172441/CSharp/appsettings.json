{
  "ConnectionStrings": {
    "DataContext": "data source={SQL_IP};initial catalog={SQL_DB};persist security info=True;user id={SQL_USER};password={SQL_PASS};multipleactiveresultsets=True;Max Pool Size=1024;"
  },
  "TrueSightLicense": "{TRUESIGHT_LICENSE}",
  "Config": {
    "Domain": "{DOMAIN}",
    "PublicRSAKey": "{PUBLIC_RSA_KEY}",
    "SecretKey": "{SECRET_KEY}"
  },
  "RabbitConfig": {
    "Hostname": "rabbitmq",
    "Username": "guest",
    "Password": "guest",
    "VirtualHost": "/",
    "Port": "5672"
  },
  "ElasticConfig": {
    "Hostname": "{ELASTIC_ENDPOINT}",
    "Username": "{ELASTIC_USERNAME}",
    "password": "{ELASTIC_PASSWORD}",
    "Prefix": "{ELASTIC_PREFIX}"
  },
  "Redis": {
    "Hostname": "{REDIS_ENDPOINT}",
    "Port": "6379",
    "Instance": "0"
  },
  "InternalServices": {
    "UTILS": "http://utils-backend:8080",
    "ES": "http://es-backend:8080",
    "Portal": "http://portal-backend:8080"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  }
}